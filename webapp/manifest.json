{
	"_version": "1.8.0",
	"sap.app": {
		"id": "yelcho.mydemo.comprouting",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"dealerPortalRemote": {
				"uri": "/sap/opu/odata/sap/YDPN",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "yelcho.mydemo.comprouting.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"componentUsages": {
			"usageComponent1": {
				"name": "yelcho.reuse.component1",
				"settings": {},
				"componentData": {
					"parentComponentName": "yelcho.mydemo.comprouting.Component"
				},
				"lazy": true
			},
			"usageComponent2": {
				"name": "yelcho.reuse.component2",
				"settings": {},
				"componentData": {
					"parentComponentName": "yelcho.mydemo.comprouting.Component"
				},
				"lazy": true
			}
		},
		"dependencies": {
			"minUI5Version": "1.65",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "yelcho.mydemo.comprouting.i18n.i18n"
				}
			},
			"": {
				"dataSource": "dealerPortalRemote",
				"defaultBindingMode": "TwoWay"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "yelcho.mydemo.comprouting.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"name": "home",
					"pattern": "",
					"target": "home"
				},
				{
					"name": "comp1",
					"pattern": "detail/:arg1:/:arg2:",
					"target": {
						"name": "reusecomp1",
						"prefix": "c1"
					}
				},
				{
					"name": "comp2",
					"pattern": "detail2",
					"target": {
						"name": "reusecomp2",
						"prefix": "c2"
					}
				},
				{
					"name": "dealers",
					"pattern": "dealers",
					"target": "dealers"
				},
				{
					"name": "dealerDetail",
					"pattern": "dealers/{dealerId}",
					"target": "dealerDetail"
				}
			],
			"targets": {
				"home": {
					"viewId": "home",
					"viewName": "Home"
				},
				"dealers": {
					"viewId": "dealers",
					"viewName": "Dealers"
				},
				"dealerDetail": {
					"viewId": "dealerDetail",
					"viewName": "DealerDetail"
				},
				"reusecomp1": {
					"type": "Component",
					"usage": "usageComponent1"
				},
				"reusecomp2": {
					"type": "Component",
					"usage": "usageComponent2"
				},
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}
